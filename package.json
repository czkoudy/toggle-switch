{
  "name": "@czkoudy/toggle-switch",
  "version": "0.0.0-development",
  "description": "Toggle switch for React",
  "author": "czkoudy",
  "homepage": "https://github.com/czkoudy/toggle-switch#readme",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": "czkoudy/toggle-switch",
  "bugs": {
    "url": "https://github.com/czkoudy/toggle-switch/issues"
  },
  "private": false,
  "keywords": [
    "React",
    "Toggle",
    "Switch"
  ],
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "uuid": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "npm run build",
    "semantic-release": "semantic-release --branches main",
    "commit": "git-cz",
    "acp": "git add . && npm run commit && git push"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@czkoudy/toggle-switch": "^1.3.4",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^3.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.37.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.7",
    "semantic-release": "^21.0.2",
    "styled-components": "^5.3.11",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vite-plugin-css-injected-by-js": "^3.1.0",
    "vite-plugin-dts": "^2.2.0"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-optional-chaining"
    ],
    "presets": [
      "@babel/preset-react"
    ]
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
